{
  "version": 2,
  "builds": [
    {
        "src": "./app.py",
        "use": "@vercel/python",
        "config": {
        "includeFiles": [
          "templates/**"
        ]
    }
    },
    {
      "src": "/static/**",
      "use": "@vercel/static"
    }

  ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.py"
        }
        
    ]
  }